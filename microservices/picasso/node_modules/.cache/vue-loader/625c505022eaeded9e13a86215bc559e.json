div id=\"flex-row\">\n                    <h2>Domain</h2>\n                    <input v-model=\"domain\" type=\"text\" placeholder=\"Leave blank for all sites\">\n                </div>\n                <select v-model=\"type\">\n                    <option disabled value=\"\">Please select one</option>\n                    <option value=\"0\">Password</option>\n                    <option value=\"1\">Credit Card</option>\n                    <option value=\"2\">Username</option>\n                    <option value=\"3\">Email</option>\n                </select>\n                <div id=\"flex-row\">\n                   <button @click=\"wasClicked=!wasClicked\" class=\"button secondary\">Cancel</button>\n                   <button @click=\"submit\" class=\"button primary\" >Submit</button>\n                </div>\n            </div>\n        </transition>\n       \n\n    </div>\n</template>\n<script>\nimport axios from \"axios\";\n\nexport default {\n    name: \"CredentialAdd\",\n    props: ['token'],\n    data () {\n        return {\n            type: 0,\n            hint: \"\",\n            val: \"\",\n            domain:\"\",\n            wasClicked: false\n        }\n    },\n    methods: {\n        submit: function() {\n            axios.request({\n                url: \"http://192.168.1.5:5000/credential/new\",\n                method: \"post\",\n                headers: {\n                    \"Authorization\": \"BEARER \" + this.token,\n                    \"Content-Type\": \"application/json\"\n                },\n                data: {\n                    \"Type\": parseInt(this.type),\n                    \"Hint\": this.hint,\n                    \"Value\": this.val,\n                    \"Domain\": this.domain\n                }\n            })\n            .then(resp => {\n                if (resp.status == 200) {\n                    this.wasClicked = false;\n                }\n            });\n        }\n    }\n}\n\n</script>\n\n<style scoped>\n@import \"../assets/css/Credential.css\";\n\n.credential-add{\n    width:100%;\n   padding: 0;\n}\n.button-primary {\n    align-self: flex-end;\n}\n#flex-row {\n    flex-grow: 1;\n    display: flex;\n    flex-flow: row;\n    justify-content: space-around;\n    align-items: center;\n}\n#cred-form {\n    \n    height: 300px;\n    display: flex;\n    flex-flow: column;\n    align-items: space-around;\n    justify-content: stretch;\n}\n\nh2 {\n    font-size: 20px;\n    color: #29434e;\n    padding: 10px;\n    min-width: 80px;\n}\ninput {\n    color: #f50057;\n    font-size: 20px;\n    height: 28px;\n    \n}\n\n.slide-f-enter-active {\n    transition: all .2s ease-out;\n}\n.slide-f-leave-active {\n    transition: all .2s ease-in;\n  }\n  .slide-f-enter {\n    transform: translateY(15px);\n    opacity: 0;\n} \n.slide-f-leave-to\n  /* .slide-fade-leave-active below version 2.1.8 */ {\n    transform: translateY(-15px);\n    opacity: 0;\n}\n\n.primary {\n    background-color: #f50057;\n    \n}\n.secondary {\n    background-color: #29434e;\n}\n\n</style>"]}]}