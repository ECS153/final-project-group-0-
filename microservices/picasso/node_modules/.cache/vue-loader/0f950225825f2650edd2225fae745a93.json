 v-else @click=\"submit\" class=\"submit\">Submit</h4>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CredentialItem\",\n  props: [\"cred\", \"type\"],\n  data() {\n    return {\n      readMode: true\n    };\n  },\n  computed: {},\n  methods: {\n    edit: function() {\n      this.readMode = false;\n    },\n    submit: function() {\n      this.readMode = true;\n      this.$http\n        .request({\n          url: \"http://192.168.1.5:5000/credential\",\n          method: \"post\",\n          headers: this.$fapiHeaders,\n          data: {\n            id: this.cred.id,\n            type: parseInt(this.cred.type),\n            domain: this.cred.domain,\n            hint: this.cred.hint\n          }\n        })\n        .then(resp => {\n          if (resp.status == 200) {\n            console.log(\"Sucess!\");\n          }\n          \n        });\n    }\n  }\n};\n</script>\n<style scoped>\n* {\n  font-size: 14px;\n  font-weight: 700;\n}\n\n#credential-item {\n  padding: 0 30px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\nh4 {\n  text-align: center;\n}\ninput {\n  margin: 10px;\n  text-align: center;\n  border: none;\n  background-color: inherit;\n  font-size: 14px;\n}\n.type {\n  width: 150px;\n}\n.domain,\n.hint {\n  width: 130px;\n}\n.edit,\n.submit {\n  width: 40px;\n}\n</style>\n"]}]}