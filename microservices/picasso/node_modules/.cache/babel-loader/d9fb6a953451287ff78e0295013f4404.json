A,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,IAAA,IAAA,CAAA;AACA,OAFA,CAAA;AAGA,KAVA;AAWA,IAAA,QAAA,EAAA,oBAAA;AACA,aAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,IAAA,IAAA,CAAA;AACA,OAFA,CAAA;AAGA,KAfA;AAgBA,IAAA,SAAA,EAAA,qBAAA;AACA,aAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,IAAA,IAAA,CAAA;AACA,OAFA,CAAA;AAGA;AApBA,GAXA;AAiCA,EAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,SAAA,KAAA,CACA,OADA,CACA;AACA,MAAA,GAAA,EAAA,kCADA;AAEA,MAAA,MAAA,EAAA,KAFA;AAGA,MAAA,OAAA,EAAA,KAAA;AAHA,KADA,EAMA,IANA,CAMA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AACA;AACA,KAVA,EAWA,KAXA,CAWA,UAAA,GAAA,EAAA;AACA,UAAA,GAAA,CAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,cAAA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA;AACA,KAjBA;AAkBA;AApDA,CAAA","sourcesContent":["<template>\n  <div id=\"credential-list\">\n    <h2 class=\"title\">Passwords</h2>\n    <div class=\"credentials card shadow-s\">\n      <div class=\"header primary\">\n        <h4>Domain</h4>\n        <h4>Hint</h4>\n      </div>\n      <credential-item\n        v-for=\"(cred, index) in pwCred\"\n        :key=\"index\"\n        :cred=\"cred\"\n      ></credential-item>\n    </div>\n\n    <h2 class=\"title\">Credit Cards</h2>\n    <div class=\"credentials card shadow-s\">\n      <div class=\"header primary\">\n        <h4>Domain</h4>\n        <h4>Hint</h4>\n      </div>\n      <credential-item\n        v-for=\"(cred, index) in ccCred\"\n        :key=\"index\"\n        :cred=\"cred\"\n      ></credential-item>\n    </div>\n\n    <h2 class=\"title\">Usernames</h2>\n    <div class=\"credentials card shadow-s\">\n      <div class=\"header primary\">\n        <h4>Domain</h4>\n        <h4>Hint</h4>\n      </div>\n      <credential-item\n        v-for=\"(cred, index) in userCred\"\n        :key=\"index\"\n        :cred=\"cred\"\n      ></credential-item>\n    </div>\n\n    <h2 class=\"title\">Emails</h2>\n    <div class=\"credentials card shadow-s\">\n      <div class=\"header primary\">\n        <h4>Domain</h4>\n        <h4>Hint</h4>\n      </div>\n      <credential-item\n        v-for=\"(cred, index) in emailCred\"\n        :key=\"index\"\n        :cred=\"cred\"\n      ></credential-item>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CredentialItem from \"@/components/Credential/Item.vue\";\n\nexport default {\n  name: \"CredentialList\",\n  components: {\n    CredentialItem\n  },\n  props: [\"token\"],\n  data() {\n    return {\n      creds: [{ hint: \"\", id: \"\", type: \"\", domain: \"\" }]\n    };\n  },\n  computed: {\n    pwCred: function() {\n      return this.creds.filter(function(cred) {\n        return cred.type == 0;\n      });\n    },\n    ccCred: function() {\n      return this.creds.filter(function(cred) {\n        return cred.type == 1;\n      });\n    },\n    userCred: function() {\n      return this.creds.filter(function(cred) {\n        return cred.type == 2;\n      });\n    },\n    emailCred: function() {\n      return this.creds.filter(function(cred) {\n        return cred.type == 3;\n      });\n    }\n  },\n  created: function() {\n    this.$http\n      .request({\n        url: \"http://localhost:8000/credential\",\n        method: \"get\",\n        headers: this.$apiHeaders\n      })\n      .then(resp => {\n        if (resp.status == 200) {\n          this.creds = resp.data;\n        }\n      })\n      .catch ((err) => {\n        if (err.response.status == 401) {\n          this.$toast.error(\"Unauthorized\");\n        } else {\n          this.$toast.error(err.response.data.title);\n        }\n      });\n  }\n};\n</script>\n\n<style scoped>\n#credential-list {\n  width: 510px;\n  display: flex;\n  flex-flow: column;\n  justify-content: stretch;\n  align-items: stretch;\n}\n.credentials {\n  width: 100%;\n  padding-bottom: 30px;\n\n}\n.header {\n  border-top-left-radius: 10px;\n  border-top-right-radius: 10px;\n  height: 40px;\n  color: white;\n  display: flex;\n  justify-content: flex-start;\n  align-items: center;\n}\n.header h4:first-child {\n  padding-left: 70px;\n  width: 230px;\n}\n</style>\n"],"sourceRoot":"src/components/Credential"}]}