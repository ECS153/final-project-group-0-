{

  "description": "Sign in safely via pi/server",
  "manifest_version": 2,
  "name": "redirect",
  "version": "1.0",
  "icons": {
    "96": "icons/pi.png"
  },

  "permissions": [
    "activeTab",
    "webRequest",
    "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icons/pi.png",
    "default_title": "redirect",
    "default_popup": "popup/option.html"
  }
}